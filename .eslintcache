s.firebaseError === \"WRONG_PASSWORD\") {\r\n  //   errorMessage = \"Та нууц үгээ шалгана уу.\";\r\n  // }\r\n\r\n  return (\r\n    <section className={classes.loginpageSection}>\r\n      <div className={classes.firstsection}>\r\n        <div className={classes.words}>\r\n          <p>Өөрийнхөө хүссэн зүйлийг</p>\r\n          <p className={classes.wordsBold}>АРИУН ЦАГААН ХӨДӨЛМӨРӨӨРӨӨ</p>\r\n          <p>олсон мөнгөөр худалдан авч бай!</p>\r\n        </div>\r\n        <div className={classes.author}>\r\n          <p>-- Жэк Ма --</p>\r\n        </div>\r\n      </div>\r\n      <div className={classes.secondsection}>\r\n        <div className={classes.form}>\r\n          <Card>\r\n            {props.logginIn && (\r\n              <div className={classes.spinner}>\r\n                <LoadingSpinner />\r\n              </div>\r\n            )}\r\n            {props.userId ? <Redirect to=\"/\" /> : null}\r\n\r\n            <div className={classes.formControl}>\r\n              <div className={classes.twoFive}>\r\n                <label htmlFor=\"email\">Э-мэйл хаяг : </label>\r\n              </div>\r\n              <div className={classes.sevenFive}>\r\n                <input\r\n                  type=\"email\"\r\n                  value={email}\r\n                  onChange={changeEmail}\r\n                  required\r\n                  placeholder=\"И-мэйл хаяг\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div className={classes.formControl}>\r\n              <div className={classes.twoFive}>\r\n                <label htmlFor=\"text\">Нууц үг : </label>\r\n              </div>\r\n              <div className={classes.sevenFive}>\r\n                <input\r\n                  type=\"password\"\r\n                  required\r\n                  value={password}\r\n                  onChange={changePassword}\r\n                  placeholder=\"Нууц үг\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div className={classes.formHorizonal}></div>\r\n            <div className={classes.formButton}>\r\n              <button onClick={loginHandler}>НЭВТРЭХ</button>\r\n            </div>\r\n\r\n            <div className={classes.formError}>{errorMessage}</div>\r\n\r\n            <div className={classes.signUpButton}>\r\n              <button>\r\n                <Link to=\"/register\">БҮРТГҮҮЛЭХ</Link>\r\n              </button>\r\n            </div>\r\n            <div className={classes.passResetLink}>\r\n              <Link to=\"/reset-password\">Нууц үг сэргээх</Link>\r\n            </div>\r\n            <div className={classes.formButton}>\r\n              {props.userId && <p>{props.userId}</p>}\r\n            </div>\r\n          </Card>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  );\r\n};\r\n\r\nconst mapStateToProps = state => {\r\n  console.log(\"LoginPage state\", state);\r\n  return {\r\n    logginIn: state.signupReducer.logginIn,\r\n    firebaseError: state.signupReducer.firebaseError,\r\n    firebaseErrorCode: state.signupReducer.firebaseErrorCode,\r\n    userId: state.signupReducer.userId\r\n  };\r\n};\r\n\r\nconst mapDispatchToProps = dispatch => {\r\n  return {\r\n    login: (email, password) => dispatch(actions.loginUser(email, password))\r\n  };\r\n};\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(LoginPage);\r\n","D:\\1.React_Project\\e-commercial-app\\src\\components\\UI\\LoadingSpinner.js",[],"D:\\1.React_Project\\e-commercial-app\\src\\components\\layout\\Layout.js",[],"D:\\1.React_Project\\e-commercial-app\\src\\components\\layout\\MainNavigation.js",[],"D:\\1.React_Project\\e-commercial-app\\src\\components\\UI\\Card.js",[],"D:\\1.React_Project\\e-commercial-app\\src\\pages\\SignUpPage.js",[],"D:\\1.React_Project\\e-commercial-app\\src\\redux\\reducer\\signupLoginReducer.js",[],"D:\\1.React_Project\\e-commercial-app\\src\\redux\\actions\\loginActions.js",["117"],"import axios from \"axios\";\r\nimport * as actions from \"./signupActions\";\r\n\r\nexport const loginUser = (email, password) => {\r\n  // console.log(email, password);\r\n  return function(dispatch) {\r\n    dispatch(loginUserStart());\r\n\r\n    const data = {\r\n      email,\r\n      password,\r\n      returnSecureToken: true\r\n    };\r\n\r\n    axios\r\n      .post(\r\n        \"https://identitytoolkit.googleapis.com/v1/accounts:signInWithPassword?key=AIzaSyAZTYNLX8i7J0HmLkL7VM7VRiCZkC5-hg4\",\r\n        data\r\n      )\r\n      .then(result => {\r\n        // LocalStorage ruu hadgalna\r\n        // console.log(\"login action data\", data);\r\n        const token = result.data.idToken;\r\n        const userId = result.data.localId;\r\n        const expiresIn = result.data.expiresIn;\r\n        const expireDate = new Date(new Date().getTime() + expiresIn * 1000);\r\n        const refreshToken = result.data.refreshToken;\r\n\r\n        localStorage.setItem(\"token\", token);\r\n        localStorage.setItem(\"userId\", userId);\r\n        localStorage.setItem(\"expireDate\", expireDate);\r\n        localStorage.setItem(\"refreshToken\", refreshToken);\r\n\r\n        dispatch(loginUserSuccess(token, userId));\r\n        // dispatch(actions.autoLogoutAfterMillisec(expiresIn * 1000));\r\n      })\r\n      .catch(err => {\r\n        dispatch(loginUserError(err));\r\n      });\r\n  };\r\n};\r\n\r\nexport const loginUserStart = () => {\r\n  return {\r\n    type: \"LOGIN_USER_START\"\r\n  };\r\n};\r\n\r\nexport const loginUserSuccess = (token, userId) => {\r\n  return {\r\n    type: \"LOGIN_USER_SUCCESS\",\r\n    token,\r\n    userId\r\n  };\r\n};\r\n\r\nexport const loginUserError = error => {\r\n  return {\r\n    type: \"LOGIN_USER_ERROR\",\r\n    error\r\n  };\r\n};\r\n","D:\\1.React_Project\\e-commercial-app\\src\\redux\\actions\\signupActions.js",[],"D:\\1.React_Project\\e-commercial-app\\src\\redux\\actions\\productsActions.js",[],"D:\\1.React_Project\\e-commercial-app\\src\\redux\\reducer\\ProductsReducer.js